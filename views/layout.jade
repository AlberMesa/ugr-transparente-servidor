html(xmlns="http://www.w3.org/1999/xhtml", lang="es", xml:lang="es")
	head
		title UGR transparente | Universidad de Granada
		meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
		meta(name="description", content="Universidad de Granada - UGR transparente")
		meta(name="keywords", content="universidad,granada,UGR transparente")
		meta(http-equiv="content-language", name="language", content="es")
		link(rel="shortcut icon", href="/img/general/favicon.ico", type="image/vnd.microsoft.icon")
		link(rel="icon", href="/img/general/favicon.ico", type="image/vnd.microsoft.icon")
		link(rel="stylesheet", id="css-style", type="text/css", href="css/style.css", media="all")

		script(type='text/javascript').
			function mostrar_menu_administracion(){
				var menu=document.getElementById("menu_administracion");
				if(menu.style.display == "none"){
					menu.style.display = "inline";
				}else
					menu.style.display = "none";
			}

			function mostrar_menu_docencia(){
				var menu=document.getElementById("menu_docencia");
				if(menu.style.display == "none"){
					menu.style.display = "inline";
				}else
					menu.style.display = "none";
			}

			function mostrar_menu_gestion(){
				var menu=document.getElementById("menu_gestion");
				if(menu.style.display == "none"){
					menu.style.display = "inline";
				}else
					menu.style.display = "none";
			}


		if seccion!=="Inicio"

			script(src="http://code.jquery.com/jquery-1.11.0.min.js")

			script(type='text/javascript' src='https://www.google.com/jsapi')

			script(src="http://jquery-csv.googlecode.com/git/src/jquery.csv.js")

			script(src="script/generar_tabla.js")

	body
		div(id="boxes")
			div(id="dialog" class="window")
				img(id="close",src="imagenes/close.png", alt="close")
				h1#titulo_tabla
				div(id="tabla")
			div(id="mask")
		div(id="contenedor_margenes" class="")
			div(id="contenedor" class="")
				div(id="cabecera" class="")
					h1#cab_inf UGR transparente
					#formularios
						a(href="http://www.ugr.es", id="enlace-ugr")
							img(src="img/general/logo-ugr.png", id="logoUGR", alt="logo UGR")

						a(href="http://osl.ugr.es", id="enlace-osl")
							img(src="imagenes/osl.jpg", id="logoOSL", alt="logo OSL")
						
						span.separador_enlaces |
						div(class="depto ejemplo_titulo")
							a(href="/", id="enlace_stack") UGR transparente
						span.separador_enlaces | 


				

				block content

				

				#banners
					.mod-banners
						ul
							li(class="banner_container model-resaltado-02 showtext")
								a(class="banner", href="http://opendata.ugr.es")
									strong Portal opendata de la UGR
									p(class="texto_banner") opendata.ugr.es es un portal para la publicación y consulta de datos abiertos de la UGR


							li(class="banner_container model-resaltado-02 showtext")
								a(class="banner", href="http://osl.ugr.es")
									strong Oficina de Software Libre
									p(class="texto_banner") Portal de la OSL de la Universidad de Granada
		

							li(class="banner_container model-resaltado-01 showtext")
								a(class="banner", href="http://okfn.org/")
									strong Open Knowledge Foundation
									p(class="texto_banner") okfn.org
				

				#interior_pie
					#pie
						a(id="WAI", href="http://www.w3.org/WAI/WCAG2AA-Conformance")
							img(src="imagenes/wcag2aa.gif", id="wcag2aa", alt="wcag2aa")
